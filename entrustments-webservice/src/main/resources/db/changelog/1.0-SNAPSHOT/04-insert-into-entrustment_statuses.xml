<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet author="hkoscielski" id="04-insert-into-entrustment_statuses">
        <insert tableName="Entrustment_Statuses">
            <column name="code">PROPOSED</column>
            <column name="name">Zaproponowane</column>
        </insert>
        <insert tableName="Entrustment_Statuses">
            <column name="code">ACCEPTED</column>
            <column name="name">Zaakceptowane</column>
        </insert>
        <insert tableName="Entrustment_Statuses">
            <column name="code">REJECTED</column>
            <column name="name">Odrzucone</column>
        </insert>
    </changeSet>

</databaseChangeLog>